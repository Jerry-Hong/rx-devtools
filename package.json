    "husky": "^0.13.3",
    "lint-staged": "^3.5.0",
    "lodash": "^4.17.4",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "prettier": "^1.3.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
